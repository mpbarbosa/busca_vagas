{
  "name": "busca_vagas_api",
  "version": "1.4.0",
  "description": "API RESTful para gerenciamento de vagas em hotéis de sindicatos",
  "type": "module",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "client": "cd client && npm start",
    "dev:all": "concurrently \"npm run dev\" \"npm run client\"",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "test:unit": "NODE_OPTIONS=--experimental-vm-modules jest tests/unit",
    "test:integration": "NODE_OPTIONS=--experimental-vm-modules jest tests/integration",
    "test:e2e": "NODE_OPTIONS=--experimental-vm-modules jest tests/e2e",
    "test:puppeteer": "node scripts/test-puppeteer.js",
    "test:puppeteer:unit": "NODE_OPTIONS=--experimental-vm-modules jest tests/unit/puppeteer-controller.test.js --verbose",
    "test:puppeteer:e2e": "NODE_OPTIONS=--experimental-vm-modules jest tests/e2e/puppeteer.test.js --verbose --testTimeout=180000",
    "test:puppeteer:business": "NODE_OPTIONS=--experimental-vm-modules jest tests/e2e/puppeteer-business-logic.test.js --verbose --testTimeout=180000",
    "test:puppeteer:all": "node scripts/run-puppeteer-tests.js",
    "test:prod": "NODE_OPTIONS=--experimental-vm-modules jest tests/production-environment-validator.test.js --verbose --testTimeout=180000",
    "validate:env": "NODE_OPTIONS=--experimental-vm-modules jest tests/production-environment-validator.test.js --verbose --testTimeout=180000",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "keywords": [
    "api",
    "rest",
    "restful",
    "busca",
    "vagas",
    "hotéis",
    "sindicatos",
    "express",
    "nodejs"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "package.json": "^2.0.1"
  },
  "devDependencies": {
    "concurrently": "^8.2.1",
    "eslint": "^8.52.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "puppeteer": "^24.31.0",
    "selenium-webdriver": "^4.15.0",
    "supertest": "^6.3.3"
  }
}
